        "satooshi/php-coveralls": ">=0.6.0",
        "psr/http-message": "1.0.*@dev",
        "zendframework/zend-loader": "~2.2.5",
        "zendframework/zend-modulemanager": "~2.2.5",
        "zendframework/zend-servicemanager": "~2.2.5"
