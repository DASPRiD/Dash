{
    "name": "dasprid/dash",
    "description": "Router prototype for Zend Framework 3",
    "license" : "BSD-2-Clause",
    "homepage": "https://github.com/DASPRiD/Dash",
    "authors": [
        {
            "name": "DASPRiD",
            "email": "mail@dasprids.de"
        }
    ],
    "require": {
        "php": ">=5.5.0",
        "psr/http-message": "~1.0",
        "zendframework/zend-stdlib": "~2.5"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0",
        "zendframework/zend-modulemanager": "~2.5|~3.0",
        "zendframework/zend-servicemanager": "dev-develop as 3.0.0",
        "squizlabs/php_codesniffer": "~2.3",
        "guzzlehttp/psr7": "~1.2"
    },
    "suggest": {
        "zendframework/zend-modulemanager": "Allows direct integration with Zend Framework applications",
        "zendframework/zend-servicemanager": "Allows use of the factories"
    },
    "autoload": {
        "psr-4": {
            "Dash\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DashTest\\": "test/"
        }
    }
}
